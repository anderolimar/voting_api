    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Service",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": ["service"],
            "env": {
                "REDIS_HOST":"localhost",
                "REDIS_PORT":"6379",
                "MONGO_URL":"mongodb://root:root@localhost:27017"
            }
        },
        {
            "name": "Launch Worker",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": ["worker"],
            "env": {
                "REDIS_HOST":"localhost",
                "REDIS_PORT":"6379",
                "MONGO_URL":"mongodb://root:root@localhost:27017"
            }
        }
    ]
}